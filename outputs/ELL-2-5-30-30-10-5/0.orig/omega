FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];
internalField   uniform 28;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 28;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 28;
        value           uniform 28;
    }

    wall
    {
        type            fixedValue;
        value           uniform 10; // Small but non-zero
    }

    symmetry
    {
        type            symmetryPlane;
    }

    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
